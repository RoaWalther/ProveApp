        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.ImagenSplashActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

