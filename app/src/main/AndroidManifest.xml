            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".Activity.RegistrarseActivity" />
        <activity android:name=".Activity.MenuPrincipalActivity" />
        <activity android:name=".Activity.RegistroActivity" />
        <activity android:name=".Activity.ClientesActivity" />
        <activity
            android:name=".Activity.TenderoActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
